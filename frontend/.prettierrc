{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": "*.xml",
      "options": {
        "parser": "xml",
        "xmlWhitespaceSensitivity": "ignore",
        "plugins": ["@prettier/plugin-xml"]
      }
    }
  ],
  "plugins": ["@prettier/plugin-xml"]
}
