{
  "name": "frontend",
  "version": "0.9.1",
  "private": true,
  "homepage": "https://researcherhojin.github.io/emelmujiro",
  "dependencies": {
    "@sentry/react": "^10.11.0",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.575.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "remark-gfm": "^4.0.1",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-helmet-async": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.39.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.16.0",
    "gh-pages": "^6.3.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "msw": "^2.11.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sitemap": "^8.0.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "npm run generate:sitemap && tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --bail=1 --no-coverage",
    "test:ci:local": "./scripts/test-ci-local.sh",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "analyze:bundle": "source-map-explorer 'build/assets/*.js'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules coverage playwright-report test-results",
    "validate": "npm run lint && npm run type-check && npm run test:coverage",
    "pre-commit": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "proxy": "http://127.0.0.1:8000",
  "overrides": {
    "nth-check": "^2.1.1",
    "svgo": "^3.0.0",
    "css-select": "^5.1.0",
    "minimatch": ">=10.2.1"
  }
}
