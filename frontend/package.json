    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "npm run generate:sitemap && tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --bail=1 --no-coverage",
    "test:ci:local": "./scripts/test-ci-local.sh",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "analyze:bundle": "source-map-explorer 'build/assets/*.js'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lighthouse": "lhci autorun",
    "lighthouse:view": "lhci open",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "analyze:build": "npm run build && npm run analyze",
    "build:profile": "react-scripts build --profile",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:sitemap": "node sitemap-generator.js",
    "postbuild": "npm run build:sitemap",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules coverage playwright-report test-results",
    "validate": "npm run lint && npm run type-check && npm run test:coverage",
    "pre-commit": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^framer-motion$": "<rootDir>/src/__mocks__/framer-motion.tsx",
      "^axios$": "axios/dist/node/axios.cjs",
      "^react-helmet-async$": "<rootDir>/src/__mocks__/react-helmet-async.tsx"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-markdown|remark-gfm|unified|bail|is-plain-obj|trough|vfile|vfile-message|unist-util-stringify-position|mdast-util-from-markdown|mdast-util-to-string|micromark|decode-named-character-reference|character-entities|mdast-util-to-markdown|zwitch|longest-streak|mdast-util-phrasing|unist-util-visit-parents|unist-util-is|mdast-util-definitions|mdast-util-to-hast|unist-util-visit|unist-util-generated|unist-util-position|unist-builder|mdast-util-gfm|mdast-util-gfm-autolink-literal|mdast-util-find-and-replace|escape-string-regexp|mdast-util-gfm-footnote|mdast-util-gfm-strikethrough|mdast-util-gfm-table|mdast-util-gfm-task-list-item|micromark-extension-gfm|micromark-extension-gfm-autolink-literal|micromark-extension-gfm-footnote|micromark-extension-gfm-strikethrough|micromark-extension-gfm-table|micromark-extension-gfm-tagfilter|micromark-extension-gfm-task-list-item|micromark-util-combine-extensions|micromark-util-chunked|micromark-util-classify-character|micromark-util-resolve-all|micromark-core-commonmark|micromark-factory-destination|micromark-factory-label|micromark-factory-space|micromark-factory-title|micromark-factory-whitespace|micromark-util-character|micromark-util-decode-numeric-character-reference|micromark-util-encode|micromark-util-normalize-identifier|micromark-util-symbol|micromark-util-decode-string|micromark-util-subtokenize|micromark-util-html-tag-name|micromark-util-sanitize-uri|trim-lines|property-information|hast-util-whitespace|space-separated-tokens|comma-separated-tokens|vfile-location|web-namespaces|micromark-util-types|hast-util-to-jsx-runtime|hast-util-from-parse5|hastscript|parse5|vfile-message|unist-util-visit|unist-util-is|zwitch|unist-util-position-from-estree|estree-util-is-identifier-name|hast-util-to-estree|estree-util-attach-comments|estree-walker|estree-util-build-jsx|estree-util-to-js|astring|source-map|periscopic|is-reference|hast-util-raw|parse5|html-void-elements|devlop|ccount|character-entities-html4|character-reference-invalid|stringify-entities|rehype-raw|hast-util-parse|inline-style-parser|style-to-object|remark-rehype|micromark-util-combine-extensions|micromark-util-html-tag-name|hast-util-from-html|remark-parse|mdast-util-to-string|remark-stringify|html-url-attributes|unist-util-visit-parents|@bundled-es-modules)/)"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "sharp-cli": "^5.2.0"
  },
  "proxy": "http://127.0.0.1:8000",
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "svgo": "^3.0.0",
    "webpack-dev-server": "^5.2.1",
    "css-select": "^5.1.0"
  }
