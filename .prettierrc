{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": "*.{yml,yaml}",
      "options": {
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": "*.{html,htm}",
      "options": {
        "parser": "html",
        "printWidth": 120,
        "tabWidth": 2,
        "htmlWhitespaceSensitivity": "css",
        "bracketSameLine": false,
        "singleAttributePerLine": false
      }
    },
    {
      "files": "*.{css,scss,less}",
      "options": {
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": "*.md",
      "options": {
        "printWidth": 80,
        "proseWrap": "preserve",
        "tabWidth": 2
      }
    },
    {
      "files": "*.{json,jsonc}",
      "options": {
        "printWidth": 100,
        "tabWidth": 2,
        "trailingComma": "none"
      }
    },
    {
      "files": "package*.json",
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "trailingComma": "none"
      }
    },
    {
      "files": "*.{js,jsx,ts,tsx}",
      "options": {
        "printWidth": 100,
        "tabWidth": 2,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always"
      }
    }
  ]
}
